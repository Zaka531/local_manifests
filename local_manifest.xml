<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  
  <remote name="pixel-devices"
          fetch="https://github.com/PixelExperience-Devices"
          revision="thirteen" />
  
  <remote name="pixel-devices-blobs"
          fetch="https://gitlab.pixelexperience.org/android/vendor-blobs"
          revision="thirteen" />

  <remote name="droidx-13"
          fetch="https://github.com/Zaka531"
          revision="droidx-13" />

  <remote name="miui-camera"
          fetch="https://gitlab.com/ThankYouMario"
          revision="uvite-sm6225" />

  <!-- Device Tree -->
     <project path="device/xiaomi/chime" name="device_xiaomi_chime" remote="droidx-13" />
  
  <!-- Vendor Tree -->
     <project path="vendor/xiaomi/chime" name="vendor_xiaomi_chime" remote="pixel-devices-blobs" />
     <project path="vendor/xiaomi/chime-perf" name="vendor_xiaomi_chime-perf" remote="pixel-devices-blobs" />
     <project path="vendor/xiaomi/citrus" name="vendor_xiaomi_citrus" remote="pixel-devices-blobs" />

  <!-- Kernel Tree -->
     <project path="kernel/xiaomi/chime" name="kernel_xiaomi_chime" remote="pixel-devices" />

  <!-- MIUI Camera -->
     <project path="vendor/xiaomi/camera" name="proprietary_vendor_xiaomi_camera" remote="miui-camera" />

  <!-- Remove repos -->
     <remove-project name="android_hardware_lineage_interfaces" />
     <remove-project name="android_packages_apps_Trebuchet" />
     <remove-project name="android_packages_overlays_Lineage" />
</manifest>
